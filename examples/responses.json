{
  "responses": [
    {
      "description": "ヘルスチェックエンドポイント",
      "method": "GET",
      "path": "/health",
      "status": 200,
      "body": {
        "status": "ok",
        "version": "1.0.0",
        "timestamp": "2025-01-01T00:00:00Z"
      }
    },
    {
      "description": "メンテナンス中のエンドポイント",
      "method": "GET",
      "path": "/maintenance",
      "status": 503,
      "body": {
        "error": "Service Unavailable",
        "message": "現在メンテナンス中です"
      }
    },
    {
      "description": "特定のメールアドレスで重複エラーを返す",
      "method": "POST",
      "path": "/users",
      "match": {
        "email": "duplicate@example.com"
      },
      "status": 409,
      "body": {
        "error": "Conflict",
        "message": "このメールアドレスは既に登録されています"
      }
    },
    {
      "description": "特定のユーザー削除を禁止",
      "method": "DELETE",
      "path": "/users/1",
      "status": 403,
      "body": {
        "error": "Forbidden",
        "message": "管理者ユーザーは削除できません"
      }
    },
    {
      "description": "ワイルドカードで管理エンドポイントを保護",
      "method": "*",
      "path": "/admin/*",
      "status": 403,
      "body": {
        "error": "Forbidden",
        "message": "管理者権限が必要です"
      }
    }
  ]
}
